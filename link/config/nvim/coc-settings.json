{
  "coc.preferences.formatOnType": true,
  "diagnostic-languageserver.formatFiletypes": { "sh": "shfmt" },
  "diagnostic-languageserver.filetypes": {
    "sh": {
      "shellcheck": {
        "args": ["--external-sources"]
      }
    }
  },
  "explorer.icon.expanded": "▼",
  "explorer.icon.collapsed": "▶",
  "explorer.width": 30,
  "explorer.keyMappingMode": "none",
  "explorer.trash.command": "trash-put %s",
  "explorer.sources": [
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.keyMappings.global": {
    "<space>f": "search",
    "<space>a": "gitStage",
    "<space>u": "gitUnstage",
    "<space>p": "preview:labeling",
    "<c-t>": "open:tab",
    "<c-s>": "open:split",
    "<c-v>": "open:vsplit",
    "<c-h>": "toggleHidden",
    "<c-r>": "refresh",
    "<c-m>": "open",
    "h": "collapse",
    "j": "nodeNext",
    "k": "nodePrev",
    "l": ["expandable?", "expand", "open"],
    "..": "gotoParent",
    "O": "expand:recursive",
    "C": "collapse:recursive",
    "cr": "rename",
    "yp": "copyFilepath",
    "yn": "copyFilename",
    "yy": "copyFile:toggle",
    "dd": "cutFile:toggle",
    "uu": "clearCopyOrCut",
    "pp": "pasteFile",
    "D": "delete",
    "at": "addFile",
    "am": "addDirectory",
    "q": "quit",
    "?": "help"
  },
  "git.splitWindowCommand": "below sp",
  "git.addGBlameToVirtualText": true,
  "git.blameUseRealTime": true,
  "go.goplsOptions": { "gofumpt": true },
  "go.checkForUpdates": "disabled",
  "list.source.files.args": ["--hidden", "--files"],
  "list.source.files.excludePatterns": ["**/.git/**"],
  "list.previewHighlightGroup": "CocPreviewHi",
  "list.previewToplineStyle": "middle",
  "list.indicator": ":",
  "list.menuAction": true,
  "list.alignColumns": true,
  "list.floatPreview": true,
  "list.nextKeymap": "<c-n>",
  "list.previousKeymap": "<c-p>",
  "list.insertMappings": {
    "<c-f>": "prompt:right",
    "<c-b>": "prompt:left",
    "<c-d>": "prompt:deletebackward",
    "<c-t>": "action:tabe",
    "<c-s>": "action:split",
    "<c-v>": "action:vsplit",
    "<c-r>": "do:refresh",
    "<c-l>": "do:previewtoggle",
    "<c-j>": "do:previewdown",
    "<c-k>": "do:previewup",
    "<c-o>": "feedkeys!",
    "<c-q>": "feedkeys!",
    "<c-g>": "feedkeys!",
    "<c-x>": "feedkeys!"
  },
  "languageserver": {
    "sql": {
      "command": "sqls",
      "args": ["-config", "$HOME/.config/sqls/config.yml"],
      "filetypes": ["sql"],
      "shell": true
    }
  },
  "leetcode.language": "golang",
  "notification.border": false,
  "notification.marginRight": 0,
  "prettier.singleQuote": false,
  "python.formatting.provider": "black",
  "suggest.maxCompleteItemCount": 16,
  "suggest.enablePreselect": false,
  "translator.engines": ["google"],
  "vetur.ignoreProjectWarning": true,
  "workspace.rootPatterns": [".git", ".gitignore"]
}
