{
    "coc.preferences.formatOnType": true,
    "coc.preferences.rootPatterns": [".git", ".gitignore", "Makefile"],
	"diagnostic.infoSign": "",
	"diagnostic.hintSign": "",
    "diagnostic.errorSign": "",
    "diagnostic.warningSign": "",
    "diagnostic-languageserver.filetypes": {
        "sh": "shellcheck"
    },
    "explorer.width": 36,
    "explorer.icon.enableNerdfont":true,
    "explorer.keyMappingMode": "none",
    "explorer.keyMappings.global": {
        "<space>f": "search",
        "<c-t>": "open:tab",
        "<c-s>": "open:split",
        "<c-v>": "open:vsplit",
        "<c-h>": "toggleHidden",
        "<c-r>": "refresh",
        "<c-m>": "open",
        "h": "collapse",
        "j": "nodeNext",
        "k": "nodePrev",
        "l": ["expandable?", "expand", "open"],
        "..": "gotoParent",
        "O": "expand:recursive",
        "C": "collapse:recursive",
        "cn": "rename",
        ",a": "gitStage",
        ",u": "gitUnstage",
        ",p": "preview:content",
        "yp": "copyFilepath",
        "yn": "copyFilename",
        "yy": "copyFile",
        "dd": "cutFile",
        "pp": "pasteFile",
        "D": "deleteForever",
        "af": "addFile",
        "ad": "addDirectory",
        "q": "quit",
        "?": "help"
    },
    "go.trace.server": "verbose",
    "git.splitWindowCommand": "below sp",
    "git.addGBlameToVirtualText": true,
    "git.blameUseRealTime": true,
    "git.branchCharacter": "",
    "leetcode.language": "golang",
    "list.source.files.args": ["--hidden", "--files"],
    "list.source.files.excludePatterns": ["**/.git/**"],
    "list.source.grep.defaultArgs": ["-smartcase", "-regex"],
    "list.indicator": ":",
    "list.alignColumns": true,
    "list.nextKeymap": "<C-n>",
    "list.previousKeymap": "<C-p>",
    "list.insertMappings": {
        "<C-f>": "prompt:right",
        "<C-b>": "prompt:left",
        "<C-y>": "prompt:paste",
        "<C-d>": "prompt:deletebackward",
        "<C-r>": "do:refresh",
        "<C-t>": "action:tabe",
        "<C-s>": "action:split",
        "<C-v>": "action:vsplit"
    },
    "list.normalMappings": {
        "<C-t>": "action:tabe",
        "<C-s>": "action:split",
        "<C-v>": "action:vsplit"
    },
    "languageserver": {
        "sql": {
            "command": "sqls",
            "args": ["-config", "$HOME/.config/sqls/config.yml"],
            "filetypes": ["sql"],
            "shell": true
        }
    },
    "suggest.enablePreselect": true,
    "suggest.maxCompleteItemCount": 20,
    "suggest.detailMaxLength": 10,
    "snippets.priority": -1,
    "snippets.ultisnips.directories": [
        "$HOME/.config/nvim/UltiSnips/"
    ],
    "snippets.userSnippetsDirectory": "$HOME/.config/nvim/UltiSnips/",
    "translator.engines": [
        "youdao"
    ]
}
