{
    "coc.preferences.colorSupport": true,
    "coc.preferences.extensionUpdateCheck": "weekly",
    "coc.preferences.rootPatterns": [".git", ".root", ".gitignore", "Makefile"],
    "codeLens.enable": true,
    "cursors.nextKey": "<C-n>",
    "cursors.cancelKey": "<Esc><Esc>",
    "cursors.previousKey": "<C-p>",
    "diagnostic-languageserver.filetypes": {
        "sh": "shellcheck"
    },
    "explorer.file.tabCD": false,
    "explorer.previewAction.onHover": false,
    "explorer.quitOnOpen": false,
    "explorer.width": 35,
    "explorer.file.column.indent.indentLine": true,
    "explorer.file.showHiddenFiles": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.git.showIgnored": true,
    "explorer.keyMappingMode": "none",
    "explorer.keyMappings.global": {
        "h": "collapse",
        "j": "nodeNext",
        "k": "nodePrev",
        "l": ["expandable?", "expand", "open"],
        "S": "toggleSelection",
        "F": "search",
        "<C-t>": "open:tab",
        "<C-s>": "open:split",
        "<C-v>": "open:vsplit",
        "<C-h>": "toggleHidden",
        "<C-r>": "refresh",
        "<Cr>": "open",
        "erd": "expand:recursive",
        "crd": "collapse:recursive",
        "crn": "rename",
        ",j": "indexNext:git",
        ",k": "indexPrev:git",
        ",a": "gitStage",
        ",u": "gitUnstage",
        ",p": "preview:labeling",
        "yy": "copyFile",
        "dd": "cutFile",
        "pp": "pasteFile",
        "dDD": "deleteForever",
        "af": "addFile",
        "ad": "addDirectory",
        "q": "quit",
        "?": "help"
    },
    "git.splitWindowCommand": "below sp",
    "git.signOffset": 99,
    "git.addedSign.hlGroup": "GitAdd",
    "git.addGBlameToVirtualText": true,
    "git.enableGlobalStatus": true,
    "git.changeRemovedSign.hlGroup": "GitDeleteTopAndBottom",
    "git.changedSign.hlGroup": "GitModify",
    "git.enableGutters": true,
    "git.removedSign.hlGroup": "GitDeleteBottom",
    "git.topRemovedSign.hlGroup": "GitDeleteTop",
    "git.changedSign.text": "~",
    "git.changeRemovedSign.text": "â‰ƒ",
    "git.addedSign.text": "+",
    "git.removedSign.text": "-",
    "javascript.referencesCodeLens.enable": true,
    "javascript.showUnused": true,
    "javascript.suggest.names": true,
    "javascript.suggestionActions.enabled": true,
    "leetcode.language": "golang",
    "list.previousKeymap": "<C-p>",
    "list.nextKeymap": "<C-n>",
    "list.source.files.args": ["--hidden", "--files"],
    "list.source.files.excludePatterns": ["**/.git/**"],
    "list.source.grep.defaultArgs": ["-smartcase", "-regex"],
    "list.indicator": "",
    "list.previewSplitRight": false,
    "list.insertMappings": {
        "<C-f>": "prompt:right",
        "<C-b>": "prompt:left",
        "<C-y>": "prompt:paste",
        "<C-d>": "prompt:deletebackward",
        "<C-r>": "do:refresh",
        "<C-c>": "do:exit",
        "<C-t>": "action:tabe",
        "<C-s>": "action:split",
        "<C-v>": "action:vsplit"
    },
    "list.normalMappings": {
        "<C-c>": "do:exit",
        "<C-t>": "action:tabe",
        "<C-s>": "action:split",
        "<C-v>": "action:vsplit"
    },
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim", ".git"],
            "filetypes": ["go"]
        },
        "bash": {
            "command": "bash-language-server",
            "args": ["start"],
            "filetypes": ["sh"],
            "ignoredRootPaths": []
        },
        "dockerfile": {
            "command": "docker-langserver",
            "filetypes": ["dockerfile"],
            "args": ["--stdio"]
        }
    },
    "python.formatting.provider": "black",
    "refactor.openCommand": "sp",
    "refactor.afterContext": 3,
    "refactor.beforeContext": 3,
    "suggest.enablePreview": true,
    "suggest.floatEnable": true,
    "suggest.detailField": "preview",
    "suggest.maxCompleteItemCount": 20,
    "suggest.maxPreviewWidth": 80,
    "suggest.triggerAfterInsertEnter": true,
    "suggest.autoTrigger": "always",
    "suggest.detailMaxLength": 30,
    "snippets.priority": -1,
    "snippets.snipmate.enable": false,
    "snippets.ultisnips.directories": [
        "UltiSnips"
    ],
    "snippets.ultisnips.enable": true,
    "snippets.ultisnips.usePythonx": true,
    "yank.enableCompletion": false
}
