map [index] q toggle_index
map a toggle_page_mode 2
map r rotate
map t toggle_index
map <C-r> reload
map <C--> zoom out
map <C-=> zoom in
map <C-n> jumplist backward
map <C-p> jumplist forward

set selection-clipboard clipboard
set guioptions ''

# Open document in fit-width mode by default
set adjust-open "best-fit"

# One page per row by default
set pages-per-row 1

#stop at page boundries
set scroll-page-aware "true"
set scroll-full-overlap 0.01
set scroll-step 100

set notification-error-bg       "#353535" # base01  # seem not work
set notification-error-fg       "#dc322f" # red
set notification-warning-bg     "#353535" # base01
set notification-warning-fg     "#dc322f" # red
set notification-bg             "#353535" # base01
set notification-fg             "#b58900" # yellow

set completion-group-bg         "#002b36" # base03
set completion-group-fg         "#839496" # base0
set completion-bg               "#000000" # base02
set completion-fg               "#ebdbb2" # base1
set completion-highlight-bg     "#353535" # base01
set completion-highlight-fg     "#eee8d5" # base2

# Define the color in index mode
set index-bg                   "#000000" # base02
set index-fg                   "#ebdbb2" # base1
set index-active-bg             "#353535" # base01
set index-active-fg             "#eee8d5" # base2

set inputbar-bg                 "#353535" # base01
set inputbar-fg                 "#eee8d5" # base2

set statusbar-bg                "#000000" # base02
set statusbar-fg                "#ebdbb2" # base1

set highlight-color             "#657b83" # base00  # hightlight match when search keyword(vim's /)
set highlight-active-color      "#268bd2" # blue

set default-bg                  "#000000" # base02
set default-fg                  "#ebdbb2" # base1
set render-loading              true
