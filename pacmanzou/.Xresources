*background:                      #ebdbb2
*foreground:                      #202124
*fading:                          8
*fadeColor:                       black
*cursorColor:                     #1ABB9B
*pointerColorBackground:          #2B2C2B
*pointerColorForeground:          #16A085

Xft.dpi:       96
Xft.antialias: true
/* Xft.hinting:   true */
/* Xft.rgba:      rgb */
/* Xft.hintstyle: hintslight */
/* Xft.lcdfilter: lcddefault */
/* Xft.autohint:  false */

!! $HOME/.Xresources
!! Input Method
URxvt.preeditType: Root
!URxvt.inputMethod: fcitx
!! Display
URxvt.depth: 32
URxvt.transparent: False
URxvt.shading: 16
!! Colors (Based on Tomorrow Night Bright)
URxvt.foreground:   #EAEAEA
URxvt.background:   #000000
URxvt.colorBD:      #FFFFFF
URxvt.colorUL:      #B9CA4A
! Black / Grey
URxvt.color0:       #000000
URxvt.color8:       #666666
! Red / Bright Red
URxvt.color1:       #D54E53
URxvt.color9:       #FF3334
! Green + Bright Green
URxvt.color2:       #B9CA4A
URxvt.color10:      #9EC400
! Yellow (Orange) + Bright Yellow (Yellow)
URxvt.color3:       #E78C45
URxvt.color11:      #E7C547
! Blue + Bright Blue
URxvt.color4:       #7AA6DA
URxvt.color12:      #7AA6DA
! MAGENTA (PURPLE) + BRIGHT MAGENTA
URxvt.color5:       #C397D8
URxvt.color13:      #B777E0
! Cyan (Aqua) + Bright Cyan
URxvt.color6:       #70C0B1
URxvt.color14:      #54CED6
! Light Grey (Selection) + White (Current Line)
URxvt.color7:       #424242
URxvt.color15:      #2A2A2A
!! URL Operations
URxvt.url-launcher: firefox
URxvt.matcher.button: 1
URxvt.perl-ext-common: default,matcher
!! Scollbar
URxvt.scrollBar: False
URxvt.scrollBar_floating: False
!! Scoll
URxvt.mouseWheelScrollPage: True
URxvt.scrollstyle: plain
URxvt.scrollTtyKeypress: True
URxvt.scrollTtyOutput: False
URxvt.scrollWithBuffer: True
URxvt.saveLines: 3000
!! Cursor
URxvt.cursorBlink: True
!! Border
URxvt.borderLess: False
!! Fonts
!URxvt.letterSpace: -1
URxvt.font: xft:WenQuanYi Micro Hei Mono:style=Regular:pixelsize=18,xft:DejaVu Sans Mono:style=Regular:antialias=True:pixelsize=18
URxvt.boldFont: xft:WenQuanYi Micro Hei Mono:style=Bold:pixelsize=18,xft:DejaVu Sans Mono:style=Bold:antialias=True:pixelsize=18

! Normal copy-paste keybindings without perls
URxvt.iso14755:                   false
URxvt.iso14755_52:                   false
URxvt.keysym.Control-V:     eval:paste_clipboard
URxvt.keysym.Shift-Control-C:     eval:selection_to_clipboard
